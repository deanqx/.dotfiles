#!/bin/sh
killall playerctld
playerctld daemon &

xrdb -merge ~/.Xresources
feh --bg-fill ~/Pictures/Wallpapers/Sunrise.JPG
~/.config/polybar/launch.sh

killall dunst
dunst &

killall picom
picom -b

setxkbmap de
setxkbmap -option caps:escape
set enable-bracketed-paste Off
xset r rate 250 35

if [ "$USER" = "deanqx" ]; then
    xrandr --output DP-4 --primary
    xrandr --output DP-4 --right-of DP-0
fi
