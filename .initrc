#!/bin/sh
# This script is launched on startup and with <Shift + CMD + r>

killall playerctld
playerctld daemon & disown

xrdb -merge ~/.Xresources
feh --bg-fill ~/pictures/wallpapers/bg_*
~/.config/polybar/launch.sh

killall dunst
dunst & disown

killall picom
picom -b

# Trying to fix not waking up bug
xset -dpms
xset s off
